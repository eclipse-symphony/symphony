[package]
name = "symphony"
version = "0.1.2"
edition = "2021"
license = "MIT"
description = "Eclipse Symphony Target Provider Rust binding"
repository = "https://github.com/eclipse-symphony/symphony.git"
keywords = ["orchestration"]
[lib]
crate-type = ["staticlib", "rlib"]

[dependencies]
libloading = "0.7"
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
sha2 = "0.10"