{
  "siteInfo": {
    "siteId": "hq",
    "currentSite": {
      "baseUrl": "",
      "username": "",
      "password": ""
    }
  },
  "api": {
    "pubsub": {
      "shared": true,
      "provider": {
        "type": "providers.pubsub.memory",
        "config": {}
      }
    },
    "vendors": [
      {
        "type": "vendors.echo",
        "route": "greetings",
        "managers": []
      },
      {
        "type": "vendors.solution",
        "loopInterval": 15,
        "route": "solution",
        "managers": [
          {
            "name": "solution-manager",
            "type": "managers.symphony.solution",
            "properties": {
              "providers.state": "mem-state",  
              "providers.target": "test-target",
              "providers.config": "mock-config",  
              "providers.secret": "mock-secret",
              "isTarget": "true",
              "targetNames": "test-target",
              "poll.enabled": "true",
              "poll.url" : "http://localhost:8082/v1alpha2/",
              "poll.user": "admin",
              "poll.password": ""
            },
            "providers": {
              "mem-state": {
                "type": "providers.state.memory",
                "config": {}
              },
              "test-target": {
                "type": "providers.target.k8s",
                "config": {
                  "inCluster": false,
                  "configType": "path",
                  "deploymentStrategy": "services"
                }
              },
              "mock-config": {
                "type": "providers.config.mock",
                "config": {}
              },
              "mock-secret": {
                "type": "providers.secret.mock",
                "config": {}
              }
            }
          }
        ]
      }
    ]
  }  
}