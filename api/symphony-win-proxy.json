    "siteId": "symphony-proxy",
    "currentSite": {
      "baseUrl": "http://localhost:8098/v1alpha2/",
      "username": "",
      "password": ""
    }
  },
  "api": {
    "vendors": [
      {
        "type": "vendors.echo",
        "route": "greetings",
        "managers": []
      },
      {
        "type": "vendors.solution",
