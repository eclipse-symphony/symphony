{
  "siteInfo": {
    "siteId": "hq",
    "currentSite": {
      "baseUrl": "",
      "username": "",
      "password": ""
    }
  },
  "api": {
    "keylock": {
      "shared": true,
      "provider": {      
        "type": "providers.keylock.memory",
        "config": {
          "mode": "Global",
          "cleanInterval" : 30,
          "purgeDuration" : 43200
        }
      }
    },
    "vendors": [
      {
        "type": "vendors.echo",
        "route": "greetings",
        "managers": []
      },
      {
        "type": "vendors.solution",
        "loopInterval": 15,
        "route": "solution",
        "managers": [
          {
            "name": "solution-manager",
            "type": "managers.symphony.solution",
            "properties": {
              "providers.persistentstate": "mem-state",
              "isTarget": "true",
              "providers.config": "mock-config",
              "providers.secret": "mock-secret",
              "providers.keylock": "mem-keylock",
              "poll.enabled": "true"
            },
            "providers": {
              "win.uwp": {
                "type": "providers.target.win10.sideload",
                "config": {
                  "name": "sideload",
		              "ipAddress": "localhost",
		              "winAppDeployCmdPath": "C:\\Program Files (x86)\\Windows Kits\\10\\bin\\10.0.22621.0\\x64\\WinAppDeployCmd.exe",
                  "silent": false,
                  "pin": ""
                }
              },
              "mem-state": {
                "type": "providers.state.memory",
                "config": {}
              },
              "mock-config": {
                "type": "providers.config.mock",
                "config": {}
              },
              "mock-secret": {
                "type": "providers.secret.mock",
                "config": {}
              },
              "mem-keylock": {
                "type": "providers.keylock.memory",
                "config": {
                  "mode" : "Shared"
                }
              }
            }
          }
        ]
      }
    ]
  },
  "bindings": [
    {
      "type": "bindings.mqtt",
      "config": {
        "brokerAddress": "tcp://localhost:1883",
        "clientID": "retail-agent",
        "requestTopic": "coa-request",
        "responseTopic": "coa-response",
        "timeoutSeconds":  30
      }
    }
  ]
}